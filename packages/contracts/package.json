{
  "name": "@blockful/contracts",
  "scripts": {
    "test": "forge test -vvv",
    "test:watch": "forge test -vvv --watch",
    "deploy:db": "forge script script/deploy/DatabaseResolver.s.sol:DatabaseResolverScript --broadcast -vvv --verify",
    "deploy:eth:l1": "forge script script/deploy/L1Resolver.s.sol:L1ResolverScript --broadcast -vvv --verify",
    "deploy:eth:l2": "forge script script/deploy/L2Resolver.s.sol --gas-estimate-multiplier 200 --broadcast -vvv --verify",
    "deploy:arb:l1": "forge script script/deploy/L1ArbitrumResolver.s.sol:L1ArbitrumResolverScript --broadcast -vvv --verify",
    "deploy:arb:l2": "forge script script/local/L2ArbitrumResolver.s.sol --broadcast -vvv --verify",
    "postinstall": "forge install",
    "lint": "forge fmt",
    "build": "forge build",
    "dev:public": "forge script script/local/PublicResolver.s.sol --rpc-url http://localhost:8545 --broadcast -vvv --private-key 0xac0974bec39a17e36ba4a6b4d238ff944bacb478cbed5efcae784d7bf4f2ff80",
    "dev:db": "forge script script/local/DatabaseResolver.s.sol --rpc-url http://localhost:8545 --broadcast -vvv --private-key 0xac0974bec39a17e36ba4a6b4d238ff944bacb478cbed5efcae784d7bf4f2ff80",
    "dev:l1": "forge script script/local/L1Resolver.s.sol --rpc-url http://localhost:8545 --broadcast -vvv --private-key 0xac0974bec39a17e36ba4a6b4d238ff944bacb478cbed5efcae784d7bf4f2ff80",
    "dev:arb:l1": "forge script script/local/L1ArbitrumResolver.s.sol --rpc-url http://127.0.0.1:8545 --broadcast -vvv --private-key 0xac0974bec39a17e36ba4a6b4d238ff944bacb478cbed5efcae784d7bf4f2ff80",
    "dev:arb:l2": "forge script script/local/L2ArbitrumResolver.s.sol --rpc-url http://127.0.0.1:8547 --broadcast -vvv --private-key 0xb6b15c8cb491557369f3c7d2c287b053eb229daa9c22138887752191c9520659"
  },
  "dependencies": {
    "@arbitrum/nitro-contracts": "^1.2.1",
    "@openzeppelin/contracts": "^4.1.0",
    "@ensdomains/buffer": "^0.1.1",
    "@ensdomains/ens-contracts": "1.1.4",
    "forge-std": "github:foundry-rs/forge-std"
  }
}
